<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EwalletPascToolbar">
        <attr name="ewallet_title" format="string" />
        <attr name="ewallet_sub_title" format="string" />
        <attr name="ewallet_title_text_size" format="dimension" />
        <attr name="ewallet_title_text_color" format="color" />
        <attr name="ewallet_sub_title_text_size" format="dimension" />
        <attr name="ewallet_sub_title_text_color" format="color" />
        <attr name="ewallet_menu_text_size" format="dimension" />
        <attr name="ewallet_menu_text_color" format="color" />
        <attr name="ewallet_back_icon" format="reference" />
        <attr name="ewallet_enable_under_divider" format="boolean" />
        <attr name="ewallet_under_divider_color" format="color" />
        <attr name="ewallet_under_divider_height" format="dimension" />
        <attr name="ewallet_support_translucent_status_bar" format="boolean" />
        <attr name="android:height" format="dimension" />
    </declare-styleable>
    <declare-styleable name="EwalletRoundTextView">
        <attr name="ewallet_rv_backgroundColor" format="color" />
        <attr name="ewallet_rv_backgroundPressColor" format="color" />
        <attr name="ewallet_rv_cornerRadius" format="dimension" />
        <attr name="ewallet_rv_strokeWidth" format="dimension" />
        <attr name="ewallet_rv_strokeColor" format="color" />
        <attr name="ewallet_rv_strokePressColor" format="color" />
        <attr name="ewallet_rv_textPressColor" format="color" />
        <attr name="ewallet_rv_isRadiusHalfHeight" format="dimension" />
        <attr name="ewallet_rv_isWidthHeightEqual" format="boolean" />
        <attr name="ewallet_rv_cornerRadius_TL" format="dimension" />
        <attr name="ewallet_rv_cornerRadius_TR" format="dimension" />
        <attr name="ewallet_rv_cornerRadius_BL" format="dimension" />
        <attr name="ewallet_rv_cornerRadius_BR" format="dimension" />
        <attr name="ewallet_rv_isRippleEnable" format="boolean" />
    </declare-styleable>


    <declare-styleable name="EwalletDatePicker">
        <attr name="ewallet_calendarViewShown" format="boolean" />
        <attr name="ewallet_endYear" format="integer" />
        <attr name="ewallet_layout" format="reference" />
        <attr name="ewallet_maxDate" format="string" />
        <attr name="ewallet_minDate" format="string" />
        <attr name="ewallet_spinnersShown" format="boolean" />
        <attr name="ewallet_startYear" format="integer" />
        <attr name="ewallet_lunarShown" format="boolean" />
        <attr name="ewallet_unitShown" format="boolean" />
    </declare-styleable>

    <declare-styleable name="EwalletRoundedImageView">
        <attr name="ewallet_riv_corner_radius" format="dimension" />
        <attr name="ewallet_riv_corner_radius_top_left" format="dimension" />
        <attr name="ewallet_riv_corner_radius_top_right" format="dimension" />
        <attr name="ewallet_riv_corner_radius_bottom_left" format="dimension" />
        <attr name="ewallet_riv_corner_radius_bottom_right" format="dimension" />
        <attr name="ewallet_riv_border_width" format="dimension" />
        <attr name="ewallet_riv_border_color" format="color" />
        <attr name="ewallet_riv_mutate_background" format="boolean" />
        <attr name="ewallet_riv_oval" format="boolean" />
        <attr name="android:scaleType" />
        <attr name="ewallet_riv_tile_mode">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="ewallet_riv_tile_mode_x">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="ewallet_riv_tile_mode_y">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="EwalletNumberPicker">
        <!-- @hide Color for the solid color background if such for optimized rendering. -->
        <attr name="ewallet_solidColor" format="color|reference" />
        <!-- @hide The divider for making the selection area. -->
        <attr name="ewallet_selectionDivider" format="reference" />
        <!-- @hide The height of the selection divider. -->
        <attr name="ewallet_selectionDividerHeight" format="dimension" />
        <!-- @hide The distance between the two selection dividers. -->
        <attr name="ewallet_selectionDividersDistance" format="dimension" />
        <!-- @hide The min height of the NumberPicker. -->
        <attr name="ewallet_internalMinHeight" format="dimension" />
        <!-- @hide The max height of the NumberPicker. -->
        <attr name="ewallet_internalMaxHeight" format="dimension" />
        <!-- @hide The min width of the NumberPicker. -->
        <attr name="ewallet_internalMinWidth" format="dimension" />
        <!-- @hide The max width of the NumberPicker. -->
        <attr name="ewallet_internalMaxWidth" format="dimension" />
        <!-- @hide The layout of the number picker. -->
        <attr name="ewallet_internalLayout" format="reference" />
        <!-- @hide The drawable for pressed virtual (increment/decrement) buttons. -->
        <attr name="ewallet_virtualButtonPressedDrawable" format="reference" />
    </declare-styleable>
</resources>